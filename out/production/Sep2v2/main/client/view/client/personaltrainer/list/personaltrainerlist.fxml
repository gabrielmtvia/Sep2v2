      </Label>
      <TableView fx:id="tableView" layoutX="53.0" layoutY="64.0" prefHeight="274.0" prefWidth="502.0">
        <columns>
          <TableColumn fx:id="fullName" prefWidth="112.0" text="Name" />
          <TableColumn fx:id="phoneNumber" prefWidth="144.0" text="Phone Number" />
            <TableColumn fx:id="date" prefWidth="116.0" text="Date" />
            <TableColumn fx:id="startTime" prefWidth="128.0" text="Start time" />
        </columns>
      </TableView>
      <Button layoutX="380.0" layoutY="352.0" mnemonicParsing="false" onAction="#onButtonBook" text="Book" />
      <Button layoutX="441.0" layoutY="352.0" mnemonicParsing="false" onAction="#viewMyBookings" text="View My Bookings" />
