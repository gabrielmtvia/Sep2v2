<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../../../tools/LoginUser.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.client.view.client.login.ClientLoginController">
   <children>
      <Label layoutX="197.0" layoutY="39.0" text="Client Login" textFill="#fbfbfb">
         <font>
            <Font size="39.0" />
         </font>
      </Label>
      <Label layoutX="54.0" layoutY="134.0" text="Personal ID:" textFill="WHITE">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label layoutX="67.0" layoutY="243.0" text="Password" textFill="WHITE">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <PasswordField fx:id="password" layoutX="204.0" layoutY="249.0" prefHeight="36.0" prefWidth="252.0" promptText="Enter your password" />
      <TextField fx:id="username" layoutX="204.0" layoutY="139.0" prefHeight="36.0" prefWidth="259.0" promptText="Enter your username" />
      <Button fx:id="login" layoutX="320.0" layoutY="313.0" mnemonicParsing="false" onAction="#onLoginButton" text="Login">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Label fx:id="systemMessage" layoutX="259.0" layoutY="361.0" text="Label" textFill="#fffdfd" />
      <Button layoutX="221.0" layoutY="315.0" mnemonicParsing="false" onAction="#onButtonBack" text="Back" />
   </children>
</AnchorPane>
